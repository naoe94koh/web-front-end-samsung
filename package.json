{
  "name": "learn-node-js-and-express",
  "version": "0.0.1",
  "description": "Node.js 런타임 환경과 Express 애플리케이션을 학습합니다.",
  "private": true,
  "engines": {
    "node": ">= 16.15.0",
    "npm": ">= 8.5.5"
  },
  "author": "yamoo9 <yamoo9@euid.dev> (https://euid.dev)",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "compile:sass": "sass --update client/src/scss:client/public/styles",
    "watch:sass": "npm run compile:sass -- --watch",
    "build:sass": "sass --style=compressed --no-source-map client/src/scss:client/public/styles",
    "compile:babel": "babel client/src/ecmascript -d client/public/scripts",
    "watch:babel": "npm run compile:babel -- --watch",
    "build:babel": "npm run compile:babel -- --minified",
    "watch": "run-p watch:*",
    "build": "run-p build:*"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@types/express": "4.17.14",
    "@types/node": "18.7.18",
    "connect-livereload": "0.6.1",
    "livereload": "0.9.3",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "sass": "1.54.9"
  },
  "dependencies": {
    "express": "4.18.1"
  }
}
